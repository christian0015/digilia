{
    "template": {
        "id": "template-3d-jsx-hybrid",
        "type": "template",
        "name": "Hybrid 3D-JSX Template",
        "metadata": {
            "created": "2025-05-08",
            "author": "Designer Pro",
            "version": "1.1.0"
        },
        "jsxComponents": [
            {
                "id": "landing-section",
                "type": "jsxContainer",
                "style": {
                    "display": "flex",
                    "flexDirection": "column",
                    "alignItems": "center",
                    "justifyContent": "center",
                    "width": "100%",
                    "height": "100vh",
                    "backgroundColor": "#121212",
                    "color": "#ffffff",
                    "overflow": "hidden",
                    "position": "relative"
                },
                "children": [
                    {
                        "id": "header-nav",
                        "type": "jsxElement",
                        "elementType": "nav",
                        "style": {
                            "position": "absolute",
                            "top": "0",
                            "left": "0",
                            "right": "0",
                            "padding": "20px",
                            "display": "flex",
                            "justifyContent": "space-between",
                            "alignItems": "center",
                            "zIndex": "100"
                        },
                        "children": [
                            {
                                "id": "logo",
                                "type": "jsxElement",
                                "elementType": "div",
                                "style": {
                                    "fontWeight": "bold",
                                    "fontSize": "24px",
                                    "letterSpacing": "2px"
                                },
                                "content": "NEXUS"
                            },
                            {
                                "id": "menu",
                                "type": "jsxElement",
                                "elementType": "div",
                                "style": {
                                    "display": "flex",
                                    "gap": "20px"
                                },
                                "children": [
                                    {
                                        "id": "menu-item-1",
                                        "type": "jsxElement",
                                        "elementType": "a",
                                        "style": {
                                            "cursor": "pointer",
                                            "textDecoration": "none",
                                            "color": "#ffffff"
                                        },
                                        "content": "Home"
                                    },
                                    {
                                        "id": "menu-item-2",
                                        "type": "jsxElement",
                                        "elementType": "a",
                                        "style": {
                                            "cursor": "pointer",
                                            "textDecoration": "none",
                                            "color": "#ffffff"
                                        },
                                        "content": "Products"
                                    },
                                    {
                                        "id": "menu-item-3",
                                        "type": "jsxElement",
                                        "elementType": "a",
                                        "style": {
                                            "cursor": "pointer",
                                            "textDecoration": "none",
                                            "color": "#ffffff"
                                        },
                                        "content": "Contact"
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "hero-content",
                        "type": "jsxElement",
                        "elementType": "div",
                        "style": {
                            "position": "relative",
                            "width": "100%",
                            "height": "100%",
                            "display": "flex",
                            "flexDirection": "column",
                            "alignItems": "center",
                            "justifyContent": "center",
                            "textAlign": "center",
                            "zIndex": "10"
                        },
                        "children": [
                            {
                                "id": "hero-title",
                                "type": "jsxElement",
                                "elementType": "h1",
                                "style": {
                                    "fontSize": "64px",
                                    "fontWeight": "bold",
                                    "marginBottom": "24px",
                                    "background": "linear-gradient(90deg, #D4AF37, #FFFFFF)",
                                    "WebkitBackgroundClip": "text",
                                    "WebkitTextFillColor": "transparent"
                                },
                                "content": "The Future Is Now"
                            },
                            {
                                "id": "hero-subtitle",
                                "type": "jsxElement",
                                "elementType": "p",
                                "style": {
                                    "fontSize": "24px",
                                    "maxWidth": "600px",
                                    "marginBottom": "40px",
                                    "lineHeight": "1.5"
                                },
                                "content": "Experience the perfect blend of reality and digital innovation with our cutting-edge technology."
                            },
                            {
                                "id": "hero-buttons",
                                "type": "jsxElement",
                                "elementType": "div",
                                "style": {
                                    "display": "flex",
                                    "gap": "16px"
                                },
                                "children": [
                                    {
                                        "id": "cta-button",
                                        "type": "jsxElement",
                                        "elementType": "button",
                                        "style": {
                                            "padding": "12px 24px",
                                            "backgroundColor": "#D4AF37",
                                            "color": "#000000",
                                            "border": "none",
                                            "borderRadius": "4px",
                                            "fontSize": "16px",
                                            "fontWeight": "bold",
                                            "cursor": "pointer"
                                        },
                                        "content": "Explore Now",
                                        "events": {
                                            "onClick": "exploreNow"
                                        }
                                    },
                                    {
                                        "id": "learn-more-button",
                                        "type": "jsxElement",
                                        "elementType": "button",
                                        "style": {
                                            "padding": "12px 24px",
                                            "backgroundColor": "transparent",
                                            "color": "#ffffff",
                                            "border": "1px solid #ffffff",
                                            "borderRadius": "4px",
                                            "fontSize": "16px",
                                            "fontWeight": "bold",
                                            "cursor": "pointer"
                                        },
                                        "content": "Learn More",
                                        "events": {
                                            "onClick": "learnMore"
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    }
}