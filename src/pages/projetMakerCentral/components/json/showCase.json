{
  "template": {
    "id": "template-3d-jsx-hybrid",
    "type": "template",
    "name": "Hybrid Key Features -JSX Template",
    "metadata": {
      "created": "2025-05-08",
      "author": "Designer Pro",
      "version": "1.1.0"
    },
    "jsxComponents": [
      {
        "id": "landing-section",
        "type": "jsxContainer",
        "style": {
          "display": "flex",
          "flexDirection": "column",
          "alignItems": "center",
          "justifyContent": "center",
          "width": "100%",
          "height": "100vh",
          "backgroundColor": "#121212",
          "color": "#ffffff",
          "overflow": "hidden",
          "position": "relative"
        },
        "children": [
          {
            "id": "showcase-section",
            "type": "jsxContainer",
            "style": {
              "padding": "100px 0",
              "backgroundColor": "#121212",
              "color": "#ffffff"
            },
            "children": [
              {
                "id": "showcase-title",
                "type": "jsxElement",
                "elementType": "h2",
                "style": {
                  "textAlign": "center",
                  "fontSize": "48px",
                  "marginBottom": "60px"
                },
                "content": "Interactive Showcase"
              },
              {
                "id": "showcase-container",
                "type": "jsxElement",
                "elementType": "div",
                "style": {
                  "width": "100%",
                  "height": "600px",
                  "position": "relative"
                },
                "canvasRef": "showcase-scene"
              },
              {
                "id": "showcase-controls",
                "type": "jsxElement",
                "elementType": "div",
                "style": {
                  "display": "flex",
                  "justifyContent": "center",
                  "gap": "20px",
                  "marginTop": "40px"
                },
                "children": [
                  {
                    "id": "view-1-button",
                    "type": "jsxElement",
                    "elementType": "button",
                    "style": {
                      "padding": "12px 24px",
                      "backgroundColor": "#D4AF37",
                      "color": "#000000",
                      "border": "none",
                      "borderRadius": "4px",
                      "cursor": "pointer",
                      "fontWeight": "bold"
                    },
                    "content": "View 1",
                    "events": {
                      "onClick": "changeView1"
                    }
                  },
                  {
                    "id": "view-2-button",
                    "type": "jsxElement",
                    "elementType": "button",
                    "style": {
                      "padding": "12px 24px",
                      "backgroundColor": "#1E90FF",
                      "color": "#000000",
                      "border": "none",
                      "borderRadius": "4px",
                      "cursor": "pointer",
                      "fontWeight": "bold"
                    },
                    "content": "View 2",
                    "events": {
                      "onClick": "changeView2"
                    }
                  },
                  {
                    "id": "view-3-button",
                    "type": "jsxElement",
                    "elementType": "button",
                    "style": {
                      "padding": "12px 24px",
                      "backgroundColor": "#00FF7F",
                      "color": "#000000",
                      "border": "none",
                      "borderRadius": "4px",
                      "cursor": "pointer",
                      "fontWeight": "bold"
                    },
                    "content": "View 3",
                    "events": {
                      "onClick": "changeView3"
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}